        "skill_config": {
          "command": "echo \"Plan drafted in c:/Users/coola/orbit_workspace/todo_app/orbit-imp. Review task.md and architecture.md, then reply 'Proceed' to continue with implementation steps.\""
        },
        "dependencies": [
          "step_2",
          "step_3"
        ]
      }
    ],
